
using UnityEngine;
using System.Collections;

public class arrays : MonoBehaviour {

    public ArrayList row;
    public float[] col;
    public float GameObject;
    //new ArrayList();


	// Use this for initialization
    void Start()
    {

        row = new ArrayList(5);
        col = new float[5];
       

        Debug.Log(row);

        int ii = 0;

        for (ii = 0; ii < col.Length; ii++)
        {
            col[ii] = 2.5f*(ii+1);
        }

        int i = 0;

        for (i = 0; i < row.Count; i++)
            row.Add(1f);
        {
            row[i] = new float[5];

        }
    }
	
	// Update is called once per frame
	void Update () {
	
	}
}
    
